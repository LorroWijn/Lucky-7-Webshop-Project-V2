@using DHDomtica.Models
<h1 class="my-4">DH-Domotica</h1>
<div class="list-group">
    @foreach (var c in (List<MainCategory>)ViewBag.AllCategories)
    {
        @Html.ActionLink(c.Name, "Pagination", "Store", new { categoryId = c.ID, pageId = 1 }, new { @class = "list-group-item child-list-group-item" })
    }
</div>