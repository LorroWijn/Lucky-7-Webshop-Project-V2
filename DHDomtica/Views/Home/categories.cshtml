@using DHDomtica.Models
@using System.Web.Mvc.Html;
@{
    ViewBag.Title = "CategoryPage";
}

<h2>@Model.Name</h2>

@foreach (var p in (List<Product>)ViewBag.AllProducts)
{
    @Html.ActionLink(p.ProductNaam, "Products", "Home", new { ID = p.ID }, new { @class = "list-group-item child-list-group-item" })
}